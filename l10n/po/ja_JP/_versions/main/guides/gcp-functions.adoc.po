#, no-wrap
msgid ""
"java -jar java-function-invoker-{gcf-invoker-version}.jar \\\n"
"  --classpath target/google-cloud-functions-1.0.0-SNAPSHOT-runner.jar \\\n"
"  --target io.quarkus.gcp.functions.QuarkusHttpFunction\n"
msgstr ""

