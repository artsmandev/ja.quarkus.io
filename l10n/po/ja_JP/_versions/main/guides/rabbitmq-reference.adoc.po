#. type: Title ===
#: upstream/_versions/main/guides/rabbitmq-reference.adoc:135
#, no-wrap
msgid "Inbound Metadata"
msgstr "インバウンドメタデータ"

#. type: Plain text
#: upstream/_versions/main/guides/rabbitmq-reference.adoc:137
msgid "Messages coming from RabbitMQ contain an instance of `IncomingRabbitMQMetadata` in the metadata."
msgstr "RabbitMQ からのメッセージには、メタデータに `IncomingRabbitMQMetadata` のインスタンスが含まれています。"

#. type: delimited block -
#: upstream/_versions/main/guides/rabbitmq-reference.adoc:149
#, no-wrap
msgid ""
"Optional<IncomingRabbitMQMetadata> metadata = incoming.getMetadata(IncomingRabbitMQMetadata.class);\n"
"metadata.ifPresent(meta -> {\n"
"    final Optional<String> contentEncoding = meta.getContentEncoding();\n"
"    final Optional<String> contentType = meta.getContentType();\n"
"    final Optional<String> correlationId = meta.getCorrelationId();\n"
"    final Optional<ZonedDateTime> creationTime = meta.getCreationTime(ZoneId.systemDefault());\n"
"    final Optional<Integer> priority = meta.getPriority();\n"
"    final Optional<String> replyTo = meta.getReplyTo();\n"
"    final Optional<String> userId = meta.getUserId();\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/rabbitmq-reference.adoc:152
#, no-wrap
msgid ""
"    // Access a single String-valued header\n"
"    final Optional<String> stringHeader = meta.getHeader(\"my-header\", String.class);\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/rabbitmq-reference.adoc:157
#, no-wrap
msgid ""
"    // Access all headers\n"
"    final Map<String,Object> headers = meta.getHeaders();\n"
"    // ...\n"
"});\n"
msgstr ""

