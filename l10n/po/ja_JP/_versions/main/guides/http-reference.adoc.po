#: upstream/_versions/main/guides/http-reference.adoc:547
msgid "`X-Forwarded-Proto`"
msgstr "`X-Forwarded-Proto`"

#. type: Plain text
#: upstream/_versions/main/guides/http-reference.adoc:548
msgid "`X-Forwarded-Host`"
msgstr "`X-Forwarded-Host`"

#. type: Plain text
#: upstream/_versions/main/guides/http-reference.adoc:549
msgid "`X-Forwarded-Port`"
msgstr "`X-Forwarded-Port`"

#. type: Plain text
#: upstream/_versions/main/guides/http-reference.adoc:550
msgid "`X-Forwarded-Ssl`"
msgstr "`X-Forwarded-Ssl`"

